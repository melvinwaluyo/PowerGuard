{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "prebuildCommand": "node scripts/setup-google-services.js"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "channel": "preview",
      "android": {
        "prebuildCommand": "node scripts/setup-google-services.js"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "prebuildCommand": "node scripts/setup-google-services.js"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
