{
  "expo": {
    "name": "PowerGuard",
    "slug": "PowerGuard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "scheme": "powerguard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/splash-icon-light.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF",
      "dark": {
        "image": "./assets/icons/splash-icon-dark.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "bundleIdentifier": "com.mlvwly.PowerGuard",
      "supportsTablet": true,
      "icon": {
        "light": "./assets/icons/Light.png",
        "dark": "./assets/icons/Dark.png",
        "tinted": "./assets/icons/Tinted.png"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "PowerGuard needs your location to set your home address for geofencing features.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/icons/adaptive-icon.png"
      },
      "splash": {
        "image": "./assets/icons/splash-icon-light.png",
        "resizeMode": "cover",
        "backgroundColor": "#FFFFFF",
        "dark": {
          "image": "./assets/icons/splash-icon-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.anonymous.PowerGuard"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "PowerGuard needs your location to set your home address for geofencing features."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "resizeMode": "cover",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b0fe4763-46db-427e-92f5-a4b73c69fc59"
      }
    },
    "owner": "mlvwly"
  }
}
